NODE_ENV=test
MONGODB_URI=mongodb://localhost:27017/test-conservatory-db
ACCESS_TOKEN_SECRET=test-access-token-secret-cascade-deletion-2023
REFRESH_TOKEN_SECRET=test-refresh-token-secret-cascade-deletion-2023
PORT=3001

# Test Configuration
USE_MEMORY_DB=true
TEST_TYPE=integration
RESET_DB_AFTER_EACH=false
LOG_LEVEL=warn

# Performance Test Configuration
PERFORMANCE_TEST_ENABLED=true
MAX_TEST_EXECUTION_TIME=30000
MAX_MEMORY_USAGE_MB=200

# MongoDB Memory Server Configuration
MONGODB_MEMORY_VERSION=6.0.0
MONGODB_MEMORY_STORAGE_ENGINE=wiredTiger
MONGODB_MEMORY_POOL_SIZE=10

# Test Data Configuration
TEST_DATA_SEED_ENABLED=false
TEST_DATA_SIZE=small